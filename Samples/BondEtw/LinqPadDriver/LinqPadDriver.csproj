  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="ConnectionDialog.xaml.cs"></Compile>
    <Compile Include="ConnectionDialog.xaml.cs">
